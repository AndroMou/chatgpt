<?xml version="1.0" encoding="utf-8"?>
<dataExtractionRules>
    <rule name="ExtractUserInput">
        <pattern>What is (.*)\?</pattern>
        <action>extract_question</action>
    </rule>
    <rule name="ExtractPhoneNumbers">
        <pattern>\b\d{3}[-.]?\d{3}[-.]?\d{4}\b</pattern>
        <action>extract_phone_number</action>
    </rule>
    <!-- Add more rules as needed -->
</dataExtractionRules>
